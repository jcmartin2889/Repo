{
	border:1px solid #00efd8;
	background-color:#e9f1f2;
	cursor:pointer;
/* Active cells */
}
td.calInputDate:active,
td.calCells:active,
td.calCellsHover:active,
td.calCellsWeekend:active,
td.calCellsWeekendHover:active,
td.calCellsExMonth:active,
td.calCellsExMonthHover:active,
td.calCellsExMonthWeekend:active,
