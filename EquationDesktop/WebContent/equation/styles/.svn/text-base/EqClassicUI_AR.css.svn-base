td.calCells.calTodayDate:active,
td.calCellsHover.calTodayDate:active,
td.calCellsWeekend.calTodayDate:active,
td.calCellsWeekendHover.calTodayDate:active,
td.calCellsExMonth.calTodayDate:active,
td.calCellsExMonthHover.calTodayDate:active,
td.calCellsExMonthWeekend.calTodayDate:active,
