{
	cursor:pointer;
/* Weekend cells */
}
td.calCellsWeekend:hover,
td.calCellsWeekendHover
{
	border:1px solid #00efd8;
	background-color:#e9f1f2;
	cursor:pointer;
/* Day outside the current month */
}
td.calCellsExMonth:hover,
