package com.misys.equation.common.dao.beans;

/**
 * GAV Record data-model.
 * 
 * @author deroset
 * 
 */
public class GAVRecordDataModel extends AbsRecord
{
	private final static String RECORD_NAME = "GAV10LF";

	private long sequenceId;
	private int sourceUnitCcsid;
	private String sourceUnit;
	private String sourceServerId;
	private String userId;
	private String workstationId;
	private int dayInMonth;
	private int timeHhmmss;
	private int sequenceNumber;
	private String type;
	private byte[] sourceGzImage;
	private byte[] sourceGsImage;
	private String apiFormat;
	private String propagationSetId;
	private String propagationRuleId;
	private String conditions;
	private String excludedFields;

	/**
	 * This is the unique class id used for serialisation.
	 */
	private static final long serialVersionUID = 1275040799051l;

	/**
	 * Default constructor
	 */
	public GAVRecordDataModel()
	{
		super();
		setEqFileName(RECORD_NAME);
	}

	public GAVRecordDataModel(long sequenceId)
	{
		this.sequenceId = sequenceId;
		setEqFileName(RECORD_NAME);
	}

	// ---getters and setters----//

	public long getSequenceId()
	{
		return this.sequenceId;
	}
	public void setSequenceId(long parameter)
	{
		this.sequenceId = parameter;
	}
	public int getSourceUnitCcsid()
	{
		return this.sourceUnitCcsid;
	}
	public void setSourceUnitCcsid(int parameter)
	{
		this.sourceUnitCcsid = parameter;
	}
	public String getSourceUnit()
	{
		return this.sourceUnit;
	}
	public void setSourceUnit(String parameter)
	{
		this.sourceUnit = parameter;
	}
	public String getSourceServerId()
	{
		return this.sourceServerId;
	}
	public void setSourceServerId(String parameter)
	{
		this.sourceServerId = parameter;
	}
	public String getUserId()
	{
		return this.userId;
	}
	public void setUserId(String parameter)
	{
		this.userId = parameter;
	}
	public String getWorkstationId()
	{
		return this.workstationId;
	}
	public void setWorkstationId(String parameter)
	{
		this.workstationId = parameter;
	}
	public int getDayInMonth()
	{
		return this.dayInMonth;
	}
	public void setDayInMonth(int parameter)
	{
		this.dayInMonth = parameter;
	}
	public int getTimeHhmmss()
