	public void setSequenceNumber(int parameter)
	{
		this.sequenceNumber = parameter;
	}
	public String getType()
	{
		return this.type;
	}
	public void setType(String parameter)
	{
		this.type = parameter;
	}
	public byte[] getSourceGzImage()
	{
		return this.sourceGzImage;
	}
	public void setSourceGzImage(byte[] parameter)
	{
		this.sourceGzImage = parameter;
	}
	public byte[] getSourceGsImage()
