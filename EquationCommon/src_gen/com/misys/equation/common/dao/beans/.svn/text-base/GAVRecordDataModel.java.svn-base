	public String getPropagationSetId()
	{
		return this.propagationSetId;
	}
	public void setPropagationSetId(String parameter)
	{
		this.propagationSetId = parameter;
	}
	public String getPropagationRuleId()
	{
		return this.propagationRuleId;
	}
	public void setPropagationRuleId(String parameter)
	{
		this.propagationRuleId = parameter;
	}
	public String getConditions()
	{
		return this.conditions;
	}
	public void setConditions(String parameter)
	{
		this.conditions = parameter;
	}
	public String getExcludedFields()
	{
		return excludedFields;
	}
	public void setExcludedFields(String excludedFields)
