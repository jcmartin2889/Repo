package com.misys.equation.common.globalprocessing.audit;

/**
 * Applicator types used in the "Applicator (Apply) Data" record.
 * 
 * @author berzosa
 */
public enum ApplyType
{
	NONE("0"), // None (extract)
	AUTO("1"), // Auto apply
	MANUAL("2"), // Manual apply
	ACKNOWLEDGE("3"), // Acknowledge
	DELETE("4"), // DELETE
	RESET("5"), // RESET
	RETRY("6"); // Acknowledge

	private String value;

	ApplyType(String value)
	{
		this.value = value;
	}

	/**
	 * Returns the code that this enum constant represents.
	 * 
	 * @return The code that this enum constant represents.
	 */
	public String getValue()
	{
		return value;
	}
}
