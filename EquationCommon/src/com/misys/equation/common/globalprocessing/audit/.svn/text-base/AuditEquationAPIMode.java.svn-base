package com.misys.equation.common.globalprocessing.audit;

/**
 * Equation API Mode used in the "Propagation Data" record.
 * 
 * @author berzosa
 */
public enum AuditEquationAPIMode
{
	AD_HOC(" "), // Ad Hoc
	ADD("A"), // Add
	MAINTAIN("M"), // Maintain
	DELETE("D"); // Delete

	private final String value;
	private AuditEquationAPIMode(String value)
	{
		this.value = value;
	}

	/**
	 * Returns the code that this enum constant represents.
	 * 
	 * @return The code that this enum constant represents.
	 */
	public String getValue()
	{
		return value;
	}
}
